# 小伍博客管理面板 - 环境变量配置
# 请根据实际部署环境修改以下配置

# API 配置
VITE_API_URL=http://localhost:8080/wp-json

# 应用配置
VITE_APP_TITLE=小伍博客管理后台
VITE_APP_LOGO=/favicon.ico

# 开发服务器配置
HOST=0.0.0.0
PORT=3000

# 功能开关
VITE_ENABLE_AI=true
VITE_ENABLE_3D_GALLERY=true
VITE_ENABLE_SMART_SEARCH=true

# CDN 配置 (可选)
VITE_CDN_URL=
VITE_CDN_ENABLED=false

# 调试模式
VITE_DEBUG=false
